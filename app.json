{
  "expo": {
    "name": "Visit Algeria",
    "slug": "visit-algeria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      }
    },

    "android": {
      "package": "com.anonymous.visitalgeria",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "GOOGLE_MAPS_API_KEY": "AIzaSyBrhAlqEr078TO0oTDbmjbwXSEfpOLNcs8",
      "router": {},
      "eas": {
        "projectId": "e3322ef5-688c-4cf8-bd03-587e9d00b71a"
      }
    }
  }
}
